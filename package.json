    "bugs": {
        "url": "https://github.com/MadProbe/iterator-helpers-polyfill/issues"
    },
    "homepage": "https://github.com/MadProbe/iterator-helpers-polyfill#readme",
    "devDependencies": {
        "rollup": "^2.56.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "typescript": "^4.4.3"
    }
