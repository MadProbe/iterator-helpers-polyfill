{
    "compilerOptions": {
        "experimentalDecorators": true,
        "module": "esnext",
        "paths": {
            "@async/*": ["./src/methods/async/*"],
            "@sync/*": ["./src/methods/sync/*"],
            "@utils/*": ["./src/utils/*"],
            "@wrappers/*": ["./src/wrappers/*"],
        },
        "sourceMap": true,
        "strict": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es2018"
    }
}